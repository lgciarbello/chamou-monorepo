            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "chamou:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/chamou/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/chamou/src/favicon.ico",
              "projects/chamou/src/assets"
            ],
            "styles": [
              "projects/chamou/src/styles/styles.scss",
              "projects/chamou/src/styles/fonts.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
